<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BildAnzeigeActivity$PlaceholderFragment">

    <TextView
        android:id="@+id/section_label"
        android:layout_width="143dp"
        android:layout_height="28dp"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:text="@string/title_activity_bild_anzeigen"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="242dp"
        android:layout_height="260dp"
        android:layout_marginBottom="75dp"
        android:layout_marginEnd="71dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="81dp"
        android:src="@color/cardview_shadow_start_color"
        app:layout_constraintBottom_toTopOf="@+id/fragment2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/section_label" />

    <fragment
        android:id="@+id/fragment2"
        android:name="de.hshl.loginregistrierung.BildAnzeigeActivity.PlaceholderFragment"
        android:layout_width="236dp"
        android:layout_height="0dp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="460dp" />

</android.support.constraint.ConstraintLayout>